{
  "name": "petrenko-yuri/omnipay-portmone",
  "type": "library",
  "description": "Portmone driver for the Omnipay payment processing library.",
  "keywords": [
    "gateway",
    "merchant",
    "portmone",
    "omnipay",
    "pay",
    "payment"
  ],
  "homepage": "https://github.com/petrenko-yuri/omnipay-portmone",
  "license": "MIT",
  "authors": [
    {
      "name": "Yuri Petrenko",
      "email": "petrenko.yuri@gmail.com"
    },
    {
      "name": "Omnipay Contributors",
      "homepage": "https://github.com/petrenko-yuri/omnipay-portmone/contributors"
    }
  ],
  "autoload": {
    "psr-4": { "Omnipay\\Portmone\\" : "src/" }
  },
  "require": {
    "omnipay/common": "^3"
  },
  "require-dev": {
    "omnipay/tests": "^3",
    "squizlabs/php_codesniffer": "^3",
    "phpro/grumphp": "^0.14"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 src/",
    "fix-style": "phpcbf -p --standard=PSR2 src/"
  },
  "prefer-stable": true
}
